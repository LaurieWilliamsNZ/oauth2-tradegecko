{
    "name": "alex-osborn/oauth2-tradegecko",
    "description": "TradeGecko OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Osborn",
            "email": "alex@superglue.co.nz",
            "homepage": "https://github.com/alex-osborn"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "bitbucket"
    ],
    "require": {
        "php": ">=5.5.0",
        "league/oauth2-client": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "AlexOsborn\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexOsborn\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
